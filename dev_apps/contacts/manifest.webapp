{
  "name": "Contacts",
  "description": "Gaia Contacts",
  "type": "certified",
  "launch_path": "/",
  "developer": {
    "name": "The Gaia Team",
    "url": "https://github.com/mozilla-b2g/gaia"
  },
  "launch_path": "/views/list/list.html",
  "name": "Contacts",
  "icons": {
    "84": "/style/icons/contacts_84.png",
    "126": "/style/icons/contacts_126.png",
    "142": "/style/icons/contacts_142.png",
    "189": "/style/icons/contacts_189.png",
    "284": "/style/icons/contacts_284.png"
  },
  "locales": {
    "ar": {
      "name": "اتصالات",
      "description": "Gaia اتصالات"
    },
    "en-US": {
      "name": "Contacts",
      "description": "Gaia Contacts"
    },
    "fr": {
      "name": "Contacts",
      "description": "Contacts Gaia"
    },
    "zh-TW": {
      "name": "聯絡人",
      "description": "Gaia 聯絡人"
    }
  },
  "permissions": {
    "themeable":{},
    "telephony":{},
    "bluetooth":{},
    "contacts":{ "access": "readwrite" },
    "mobileconnection":{},
    "settings":{ "access": "readwrite" },
    "desktop-notification":{},
    "alarms": {},
    "systemXHR": {},
    "idle":{},
    "device-storage:sdcard": { "access": "readcreate" },
    "phonenumberservice": {},
    "nfc-share":{},
    "nfc":{},
    "moz-extremely-unstable-and-will-change-webcomponents":{}
  },
  "orientation": "default",
  "activities": {
    "pick": {
      "filters": {
        "type": {
          "required": true,
          "value": ["webcontacts/contact","webcontacts/email",
                    "webcontacts/tel","webcontacts/select",
                    "text/vcard", "text/x-vcard", "text/directory"]
         }
       },
      "disposition": "inline",
      "href": "/views/list/list.html?action=pick",
      "returnValue": true
    },
    "import": {
      "filters": {
        "type": {
          "required": true,
          "value": ["text/vcard", "text/x-vcard", "text/directory"]
        }
      },
      "disposition": "inline",
      "href": "/views/vcard_load/vcard_load.html",
      "returnValue": true
    },
    "open": [
    {
      "filters": {
        "type": ["webcontacts/contact"]
      },
      "disposition": "inline",
      "href": "/views/details/details.html",
      "returnValue": true
    },
    {
      "filters": {
        "type": ["text/vcard", "text/x-vcard", "text/directory"]
      },
      "disposition": "inline",
      "href": "/views/vcard_load/vcard_load.html",
      "returnValue": true
    }
    ],
    "new": {
      "filters": {
        "type": "webcontacts/contact"
      },
      "disposition": "inline",
      "href": "/views/form/form.html?action=new",
      "returnValue": true
    },
    "update": {
      "filters": {
        "type": "webcontacts/contact"
      },
      "disposition": "inline",
      "href": "/views/form/form.html?action=update",
      "returnValue": true
    }
  },
  "redirects": [
    {"from": "facebook",
    "to": "/shared/pages/import/redirects/redirect.html"},
    {"from": "facebook_dialogs",
    "to": "/shared/pages/import/redirects/dialogs_end.html"},
    {"from": "facebook_logout",
    "to": "/shared/pages/import/redirects/dialogs_end.html"},
    {"from": "live",
    "to": "/shared/pages/import/redirects/redirect.html"},
    {"from": "gmail",
    "to": "/shared/pages/import/redirects/redirect.html"}
  ],

  "datastores-owned": {
    "ice_contacts": {
      "description": "Contains references to in case of emergency contacts"
    },
    "Import_Status_Data": {
      "description": "Import status data shared between FTU and Contacts"
    }
  },
  "default_locale": "en-US"
}
